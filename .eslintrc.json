{
  "extends": [
    "next/core-web-vitals", 
    "prettier",
    "next",
    "plugin:react-hook-form/recommended"
  ],
  "plugins": [
    "react-hook-form"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": [ 
      "error",
      {
        "staticHooks":{
          "useForm": true
        }
      }
    ],
    "react-hook-form/no-access-control": "error"
  }

}
